{
    "name": "rodrifarias/password-generator",
    "type": "library",
    "keywords": ["password", "random-password", "password-generator"],
    "version": "0.1.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rodrifarias\\PasswordGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Rodrigo Farias",
            "email": "rodrigo.campos.farias@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.21"
    },
    "scripts": {
        "test": "phpunit",
        "test:with-covarage": "php -dxdebug.mode=coverage vendor/bin/phpunit --coverage-clover='covarage.xml'"
    }
}
